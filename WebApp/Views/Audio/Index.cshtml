                                                        @{
                                                            <img src="@lstAudioDeXuat[itemAU].Image" alt="" class="avatar-sm-custom  rounded">
                                                        }
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            }
                        </ul>
                    </div>
                }
            }

